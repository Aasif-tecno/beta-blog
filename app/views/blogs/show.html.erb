<div class="page-header">
  <%= link_to blogs_path, class: 'btn btn-default' do %>
    All Blogs
  <% end %>
  <%= link_to edit_blog_path(@blog), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show blog</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @blog.title %></dd>

  <dt>Description:</dt>
  <dd><%= @blog.description %></dd>

  <dt>Published:</dt>
  <dd><%= @blog.published %></dd>

  <dt>Published at:</dt>
  <dd><%= @blog.published_at %></dd>

  <dt>User:</dt>
  <dd><%= @blog.user %></dd>

</dl>
