<div class="page-header">
  <%= link_to discussions_path, class: 'btn btn-default' do %>
    All Discussions
  <% end %>
  <%= link_to edit_discussion_path(@discussion), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show discussion</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @discussion.user %></dd>

  <dt>Title:</dt>
  <dd><%= @discussion.title %></dd>

</dl>
